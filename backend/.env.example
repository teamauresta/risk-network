# Application
DEBUG=false
APP_NAME=Risk Network Analyzer

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/risknetwork

# Redis (for caching)
REDIS_URL=redis://localhost:6379/0

# NLP
EMBEDDING_MODEL=all-MiniLM-L6-v2

# Clustering defaults
MIN_CLUSTER_SIZE=3
SIMILARITY_THRESHOLD=0.4

# CORS (comma-separated origins)
CORS_ORIGINS=["http://localhost:3000","http://localhost:5173"]
